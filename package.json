{
  "name": "tov-template",
  "version": "1.17.0",
  "description": "vite + vue3 + ts 开箱即用现代开发模板",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "build": "vite build",
    "prepare": "husky install",
    "dev:host": "vite --host",
    "dev:open": "vite --open",
    "preview": "vite preview",
    "coverage": "vitest --coverage",
    "typecheck": "npx vue-tsc --noEmit",
    "preview:host": "vite preview --host",
    "preview:open": "vite preview --open",
    "lint": "eslint --ext .ts,.js,.jsx,.vue .",
    "release": "plop --plopfile scripts/release.cjs",
    "auto:remove": "plop --plopfile scripts/remove.cjs",
    "auto:create": "plop --plopfile scripts/create.cjs",
    "lint:fix": "eslint --fix --ext .ts,.js,.jsx,.vue .",
    "build:debug": "cross-env NODE_ENV=debug vite build",
    "deps:fresh": "plop --plopfile scripts/deps-fresh.cjs"
  },
  "engines": {
    "node": ">=v18.16.0"
  },
  "packageManager": "pnpm@8.6.0",
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.11.0",
    "@types/ityped": "^1.0.1",
    "@types/node": "^18.16.16",
    "@typescript-eslint/parser": "^5.59.8",
    "@unocss/eslint-config": "^0.53.0",
    "@unocss/reset": "^0.52.7",
    "@unocss/transformer-directives": "^0.52.7",
    "@vitejs/plugin-legacy": "^4.0.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vueuse/components": "^10.1.2",
    "@vueuse/core": "^10.1.2",
    "@vueuse/integrations": "^10.1.2",
    "axios": "^1.4.0",
    "c8": "^7.14.0",
    "changelogen": "^0.5.3",
    "cross-env": "^7.0.3",
    "echarts": "^5.4.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.14.1",
    "husky": "^8.0.3",
    "ityped": "^1.0.3",
    "lint-staged": "^13.2.2",
    "local-pkg": "^0.4.3",
    "markdown-it-prism": "^2.3.0",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.3",
    "pinia-plugin-persistedstate": "^3.1.0",
    "plop": "^3.1.2",
    "prettier": "^2.8.8",
    "prism-theme-vars": "^0.2.4",
    "taze": "^0.10.1",
    "typescript": "^5.1.3",
    "unocss": "^0.52.7",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.25.0",
    "unplugin-vue-router": "^0.6.4",
    "vite": "^4.3.9",
    "vite-auto-import-resolvers": "^3.0.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-env-types": "^0.1.3",
    "vite-plugin-mock": "^2.9.8",
    "vite-plugin-removelog": "^0.2.1",
    "vite-plugin-use-modules": "^1.4.5",
    "vite-plugin-vue-devtools": "^0.0.22",
    "vite-plugin-vue-markdown": "^0.23.5",
    "vite-plugin-vue-meta-layouts": "^0.2.2",
    "vite-plugin-warmup": "^0.1.0",
    "vitest": "^0.31.4",
    "vue": "^3.3.4",
    "vue-dark-switch": "^1.0.0",
    "vue-echarts": "^6.5.5",
    "vue-i18n": "^9.2.2",
    "vue-request": "2.0.2",
    "vue-router": "^4.2.2",
    "vue-toastification": "2.0.0-rc.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "eslint --cache --fix"
  },
  "resolutions": {
    "vue-demi": "npm:vue-demi@v0.14.5",
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec"
  },
  "repository": {
    "url": "https://github.com/dishait/tov-template"
  }
}